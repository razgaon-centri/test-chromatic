{
  "npmScope": "raz",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "general": {
      "tags": []
    },
    "general-e2e": {
      "tags": [],
      "implicitDependencies": ["general"]
    },
    "raz": {
      "tags": []
    },
    "raz-e2e": {
      "tags": [],
      "implicitDependencies": ["raz"]
    }
  }
}
